import java.util.Scanner;

public class TablaMultiplicar {

    public static void main(String[] args) {
        int x;
        Scanner sc = new Scanner(System.in);
        System.out.println("Numero a multiplicar: ");
        x = sc.nextInt();
        sc.close();
        for(int i=1;i<=10;i++){
            System.out.println(x+" x "+i+" = "+x*i);
        }
    }
}
